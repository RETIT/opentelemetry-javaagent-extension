<?xml version="1.0" encoding="UTF-8"?>
<!-- ${project.groupId}:${project.artifactId}:${project.version} -->
<FindBugsFilter>
  <Match>
      <Class name="~.*_jmh.*" />
  </Match>
  <Match>
      <Bug code="NM" />
      <Class name="~.*Assertions.*" />
  </Match>
  <Match>
      <Bug code="CD" />
      <Class name="~.*" />
  </Match>
  <Match>
      <Class name="~.*Messages" />
  </Match>
  <Match>
      <Class name="~.*Assert" />
  </Match>
  <Match>
      <Bug code="UwF, SIC, NP, Dm, UI, RV, DMI, EI, THROWS" />
      <Class name="~.*(Test|Benchmark).*" />
  </Match>
  <Match>
    <Bug category="SECURITY"/>
    <Class name="~.*(Test|Benchmark).*" />
  </Match>
  <Match>
    <Bug pattern="DESERIALIZATION_GADGET, FORMAT_STRING_MANIPULATION, PATH_TRAVERSAL_IN, WEAK_FILENAMEUTILS, RCN_REDUNDANT_NULLCHECK_OF_NONNULL_VALUE, EI_EXPOSE_REP, EI_EXPOSE_REP2"/>
  </Match>
  <Match>
    <Bug code="THROWS"/>
  </Match>
</FindBugsFilter>
